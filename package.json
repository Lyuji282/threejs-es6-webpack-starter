{
  "name": "threejs-es6-webpack-starter",
  "description": "A starter project for Three.js with support for ES6 and Webpack 4",
  "author": "jackdbd <jackdebidda@gmail.com> (http://www.giacomodebidda.com/)",
  "homepage": "https://jackdbd.github.io/threejs-es6-webpack-starter",
  "repository": "git@github.com:jackdbd/threejs-es6-webpack-starter.git",
  "version": "5.0.0",
  "main": "./src/js/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn prebuild && webpack --config webpack.config.js --progress --mode production",
    "ci": "yarn test && yarn coverage && yarn build",
    "coverage": "codecov",
    "deploy": "gh-pages -d build",
    "dev": "webpack-dev-server --config webpack.config.js --mode development --open",
    "format": "prettier --write '{,!(node_modules)/}**/*.{js,jsx}'",
    "lint:css": "stylelint --config .stylelintrc.json --formatter verbose './src/**/*.{css,js,jsx,ts,tsx}'",
    "ncu": "ncu",
    "ncuu": "ncu --upgrade",
    "nuke": "rimraf node_modules && rm yarn.lock",
    "prebuild": "yarn lint:css",
    "predeploy": "yarn test && yarn build",
    "static": "chromium-browser build/index.html",
    "stats": "webpack-bundle-analyzer build/stats.json --port 8888",
    "test": "jest --verbose --no-cache",
    "updtr": "updtr --use yarn --test-stdout",
    "webhint": "hint https://jackdbd.github.io/threejs-es6-webpack-starter/",
    "webhint:report": "python -m webbrowser ~/repos/threejs-es6-webpack-starter/hint-report/https-jackdbd-github-io-threejs-es6-webpack-starter/index.html"
  },
  "dependencies": {
    "orbit-controls-es6": "2.0.0",
    "three": "0.108.0",
    "three.interaction": "0.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@hint/configuration-web-recommended": "^7.1.0",
    "@packtracker/webpack-plugin": "2.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.0.0",
    "css-loader": "3.2.0",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "favicons-webpack-plugin": "1.0.2",
    "file-loader": "4.2.0",
    "gh-pages": "2.1.1",
    "hint": "^5.2.2",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "6.0.0",
    "jest": "24.9.0",
    "jest-dom": "3.1.2",
    "mini-css-extract-plugin": "0.8.0",
    "npm-check-updates": "3.1.22",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "updtr": "^3.1.0",
    "webpack": "4.39.3",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.8",
    "webpack-dev-server": "3.8.0",
    "webpack-glsl-loader": "1.0.1"
  },
  "keywords": [
    "babel",
    "boilerplate",
    "es6",
    "threejs",
    "three.js",
    "webpack",
    "webgl"
  ],
  "bugs": {
    "url": "https://github.com/jackdbd/threejs-es6-webpack-starter/issues"
  }
}
